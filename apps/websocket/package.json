{
  "name": "@pyxl/websocket",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env tsc",
    "clean": "git clean -xdf .turbo node_modules",
    "dev": "dotenv -v PORT=3001 tsx watch src/wssDevServer.ts --tsconfig tsconfig.server.json ",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env node dist/prodServer.js",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@pyxl/api": "^0.1.0",
    "@pyxl/auth": "^0.1.0",
    "@pyxl/db": "^0.1.0",
    "@trpc/client": "^10.20.0",
    "@trpc/next": "^10.20.0",
    "@trpc/react-query": "^10.20.0",
    "@trpc/server": "^10.20.0",
    "superjson": "1.9.1",
    "tsx": "^3.12.3",
    "zod": "^3.21.4",
    "ws": "^8.3.0",
    "eventemitter3": "^5.0.0"
  },
  "devDependencies": {
    "@pyxl/eslint-config": "^0.1.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.38.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  }
}
